			<HostAddress>msk.vpn.cognyte.com</HostAddress>
			<BackupServerList>
				<HostAddress>thc.vpn.cognyte.com</HostAddress>
			</BackupServerList>
		</HostEntry>
		<HostEntry>
			<HostName>Israel - Backup</HostName>
			<HostAddress>thc.vpn.cognyte.com</HostAddress>
		</HostEntry>
		<HostEntry>
			<HostName>US-West</HostName>
			<HostAddress>az-us.vpn.cognyte.com</HostAddress>
		</HostEntry>
		<HostEntry>
			<HostName>Brazil</HostName>
			<HostAddress>az-br.vpn.cognyte.com</HostAddress>
		</HostEntry>
		<HostEntry>
